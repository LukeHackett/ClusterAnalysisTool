<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <!-- Name of the Document (can be empty) -->
    <name>DOCUMENT NAME</name>
    <!-- Start Basic Style -->
    <Style id="BasicStyle">
      <IconStyle>
        <color>FFFFFFFF</color>
        <scale>1.2</scale>
        <Icon>
          <href>http://google.com/mapfiles/ms/micons/red.png</href>
        </Icon>
      </IconStyle>
    </Style>
    <!-- End Basic Style -->
    <!-- Start Placemarks -->
    <Placemark>
      <styleUrl>#BasicStyle</styleUrl>
      <Point>
        <!-- Longitude, Latitude, Altitude (No Spaces) -->
        <coordinates>-0.2341408,51.3653237,0</coordinates>
      </Point>
      <!-- Extended Data must preserve this order -->
      <ExtendedData>
        <!-- Device Name -->
        <SimpleData name="device">9800</SimpleData>
        <!-- Device PIN -->
        <SimpleData name="pin">03dcde00</SimpleData>
        <!-- Time stamp of the event -->
        <SimpleData name="timestamp">07/08/2012 00:00:00<SimpleData>
        <!-- Reference device? (true/false) -->
        <SimpleData name="reference">true</SimpleData>          
        <!-- Event type (drop/fail/success - success not implemented) -->
        <SimpleData name="type">drop</SimpleData>
        <!-- Device Start RAT -->
        <SimpleData name="start_rat">2G</SimpleData>
        <!-- Device End RAT -->
        <SimpleData name="end_rat">3G</SimpleData>
        <!-- Device Start Mix-Band -->
        <SimpleData name="start_mix_band">Freq1</SimpleData>
        <!-- Device End Mix-Band -->
        <SimpleData name="end_mix_band">Freq2</SimpleData>
        <!-- Device Start RRC State -->
        <SimpleData name="start_rrc_state">IDLE<SimpleData>
      </ExtendedData>
    </Placemark>
    <!-- End Placemarks -->
  </Document>
</kml>